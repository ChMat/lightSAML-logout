{
    "name": "lightsaml/lightsaml-logout",
    "license": "GPL-3.0+",
    "type": "library",
    "description": "Light SAML2 logout php library",
    "autoload": {
        "psr-0": {
            "LightSaml\\Logout\\Tests\\": "tests/",
            "LightSaml\\Logout\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.1",
        "lightsaml/lightsaml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "monolog/monolog": "~1.3"
    },
    "scripts": {
        "post-install-cmd": [
            "bash contrib/setup.sh"
        ],
        "post-update-cmd": [
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
